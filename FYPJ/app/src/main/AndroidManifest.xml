<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.aaronhu.fypj">
    <uses-permission android:name="android.permission.INTERNET" />
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme.NoActionBar">


        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name=".StudyActivity"
            android:label="@string/title_activity_study"
            android:theme="@style/AppTheme.NoActionBar"></activity>


        <activity
        android:name=".BookActivity"
        android:label="BookActivity"
        android:theme="@style/AppTheme.NoActionBar"></activity>

        <activity
        android:name=".ExamActivity"
        android:label="ExamActivity"
        android:theme="@style/AppTheme.NoActionBar"></activity>

        <activity
            android:name=".ResultActivity"
            android:label="ResultActivity"
            android:theme="@style/AppTheme.NoActionBar"></activity>

        <activity
            android:name=".MultiChoiceActivity"
            android:label="MultiChoiceActivity"
            android:theme="@style/AppTheme.NoActionBar"></activity>
</application>


</manifest>
